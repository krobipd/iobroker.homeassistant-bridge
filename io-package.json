{
  "common": {
    "name": "homeassistant-bridge",
    "version": "0.3.0",
    "news": {
      "0.3.0": {
        "en": "Code cleanup: removed uuid/body-parser deps, fixed mDNS XML bug, session cleanup, DRY refactor",
        "de": "Code-Cleanup: uuid/body-parser entfernt, mDNS XML-Bug gefixt, Session-Cleanup, DRY-Refactor"
      },
      "0.2.0": {
        "en": "Simplified: proxy removed, Avahi-only mDNS, CORS removed",
        "de": "Vereinfacht: Proxy entfernt, nur Avahi für mDNS, CORS entfernt"
      },
      "0.1.0": {
        "en": "Initial release",
        "de": "Erstveröffentlichung"
      }
    },
    "title": "Home Assistant Bridge",
    "titleLang": {
      "en": "Home Assistant Emulation Bridge",
      "de": "Home Assistant Emulation Bridge"
    },
    "desc": {
      "en": "Emulates a minimal Home Assistant server so devices expecting a Home Assistant dashboard can display any custom web URL.",
      "de": "Emuliert einen minimalen Home Assistant Server, sodass Geräte mit Home-Assistant-Dashboard-Unterstützung beliebige Web-URLs anzeigen können."
    },
    "authors": [
      "krobi"
    ],
    "keywords": [
      "homeassistant",
      "shelly",
      "wall display",
      "vis",
      "bridge"
    ],
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "main": "main.js",
    "icon": "homeassistant-bridge.png",
    "enabled": true,
    "loglevel": "info",
    "mode": "daemon",
    "type": "infrastructure",
    "compact": true,
    "connectionType": "local",
    "dataSource": "push",
    "adminUI": {
      "config": "materialize"
    },
    "dependencies": [
      {
        "js-controller": ">=5.0.0"
      }
    ],
    "globalDependencies": [
      {
        "admin": ">=6.0.0"
      }
    ]
  },
  "native": {
    "port": 8123,
    "visUrl": "",
    "authRequired": false,
    "username": "admin",
    "password": "",
    "mdnsEnabled": true,
    "serviceName": "ioBroker"
  },
  "objects": [],
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Server is running",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "info.clients",
      "type": "state",
      "common": {
        "role": "value",
        "name": "Connected Shelly Displays",
        "type": "number",
        "read": true,
        "write": false,
        "def": 0
      },
      "native": {}
    }
  ]
}